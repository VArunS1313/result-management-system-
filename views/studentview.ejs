<%- include('include/_header')%>
<main id="site-main">
    <div class="container">
        <div class="box-nav d-flex justify-between">
            <div class="filter">
                <div class="box-nav d-flex justify-between">
                    <a href="/" class="boarder-shadow">
                      
                        <span class="text-gradient">Back </span>
                    </a>
                </div>
        </div>
        </div>
        <div class="form-title text-center">
            <h2 class="text-dark">Search Result</h2>

        </div>
        <form method="POST" action="/api/student_search"  id="result_student">
            <div class="new_student">
               
                <div class="form-group">
                    <label for="rollno" class="text-light">Roll no</label>
                    
                    <input type="number" name="rollno" value="" placeholder="12">
                </div>
                <div class="form-group">
                    <label for="dateofbirth" class="text-light">Date of birth</label>
                    
                    <input type="text" name="dateofbirth" value="" placeholder="DDMMYYYY" required>
                </div>
                <div class="form-group">
                    <button type="submit" class="btn text-dark update">Save</button>
                </div>
            </div>
            

          
        </form>

    </div>

</main>
<%- include('include/_footer')%>